{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to dev-server",
      "port": 9228,
      "restart": true,
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${workspaceFolder}/src/*",
        "webpack:////src/*": "${workspaceFolder}/src/*",
        "webpack:///./src/*": "${workspaceFolder}/src/*"
      }
    }
  ]
}